{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["mocha"],
  "env": {
    "mocha": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "mocha/no-exclusive-tests": 2,
    "global-require": 0,
    "no-underscore-dangle": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module-alias": {},
      "webpack": {}
    }
  }
}
