{
  "stage": 0,
  "plugins": ["typecheck"],
  "optional": ["runtime"],
  "env": {
    "development": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": [
          {
            "target": "react-transform-webpack-hmr",
            "imports": ["react"],
            "locals": ["module"]
          },
          {
            "target": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }
        ]
      }
    }
  }
}
